package basics;

public class nestedLoop {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int k=3;
		for(int i=1;i<4;i++)
		{
			
			for(int j=i;j<i+i;j++)
			{
				int s=k*j;
				System.out.print(s);
				System.out.print("\t");
				
			}
			System.out.println("");
		}

	}

}
