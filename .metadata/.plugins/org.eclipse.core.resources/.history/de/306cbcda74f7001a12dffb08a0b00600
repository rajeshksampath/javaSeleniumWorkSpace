package basics;

public class nestedLoop {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int k=3;
		for(int i=1;i<4;i++)
		{
			
			for(int j=1;j<i+1;j++)
			{
				int s=k*(j+1);
				System.out.print(s);
				System.out.print("\t");
			}
			System.out.println("");
		}

	}

}
